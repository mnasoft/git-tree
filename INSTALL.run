#!/bin/bash

echo "" > INSTALL.log
echo "" > UNINSTALL.run

mkdir -p /usr/local/bin/
git clean -fx
for i in `find . -name "git-*" -o -name "clp-*" -o -name "pi-*"`
do
    echo ${i}
    cp -f ${i} /usr/local/bin/
    echo "/usr/local/bin/${i}" >> INSTALL.log
    echo "rm -f /usr/local/bin/${i}" >> UNINSTALL.run
    chmod +x UNINSTALL.run
done
