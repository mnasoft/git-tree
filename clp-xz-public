#!/bin/bash
#
# Выполняет перенос в каталог ~/public_html/Common-Lisp-Programs/Archive/ архивов git.tar.xz 

clp-check-public
####################
# xz.tar
####################
clp-local-remake-git
clp-local-remake-xz
cd ~/xz
tar -cvvf xz.tar *.git.tar.xz
mv xz.tar ~/public_html/Common-Lisp-Programs/Archive/
