#!/bin/bash
#
# Выполняет перенос на сайт pi пакетов и файлов синхронизации,
# необходимях для обновления msys2.

clp-check-public
####################
# xz.tar
####################
clp-local-remake-git
clp-local-remake-xz
cd ~/xz
tar -cvvf xz.tar *.git.tar.xz
mv xz.tar ~/public_html/Common-Lisp-Programs/Archive/
